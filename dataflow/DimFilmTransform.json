{
	"name": "DimFilmTransform",
	"properties": {
		"folder": {
			"name": "Project"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "saw-uofu-is6480-dldwhndev-001-WorkspaceDefaultStorage",
						"type": "LinkedServiceReference"
					},
					"name": "sourceCategory"
				},
				{
					"linkedService": {
						"referenceName": "saw-uofu-is6480-dldwhndev-001-WorkspaceDefaultStorage",
						"type": "LinkedServiceReference"
					},
					"name": "sourceFilmCategory"
				},
				{
					"linkedService": {
						"referenceName": "saw-uofu-is6480-dldwhndev-001-WorkspaceDefaultStorage",
						"type": "LinkedServiceReference"
					},
					"name": "sourceFilm"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "saw-uofu-is6480-dldwhndev-001-WorkspaceDefaultStorage",
						"type": "LinkedServiceReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "join1"
				},
				{
					"name": "select1"
				},
				{
					"name": "lookup1"
				},
				{
					"name": "surrogateKey1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          category_id as short,",
				"          name as string,",
				"          last_update as string",
				"     ),",
				"     useSchema: false,",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delimited',",
				"     fileSystem: 'fsuofuis6480dldw001',",
				"     folderPath: 'IS6480_DW_GroupSix_Project/category',",
				"     fileName: 'data.csv',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: true) ~> sourceCategory",
				"source(output(",
				"          film_id as short,",
				"          category_id as short,",
				"          last_update as string",
				"     ),",
				"     useSchema: false,",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delimited',",
				"     fileSystem: 'fsuofuis6480dldw001',",
				"     folderPath: 'IS6480_DW_GroupSix_Project/film_category',",
				"     fileName: 'data.csv',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: true) ~> sourceFilmCategory",
				"source(output(",
				"          film_id as short,",
				"          title as string,",
				"          description as string,",
				"          release_year as short,",
				"          language_id as boolean,",
				"          rental_duration as short,",
				"          rental_rate as double,",
				"          length as short,",
				"          replacement_cost as double,",
				"          rating as string,",
				"          last_update as string,",
				"          special_features as string,",
				"          fulltext as string",
				"     ),",
				"     useSchema: false,",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delimited',",
				"     fileSystem: 'fsuofuis6480dldw001',",
				"     folderPath: 'IS6480_DW_GroupSix_Project/film',",
				"     fileName: 'data.csv',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: true) ~> sourceFilm",
				"sourceFilmCategory, sourceCategory join(sourceFilmCategory@category_id == sourceCategory@category_id,",
				"     joinType:'outer',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 select(mapColumn(",
				"          film_id,",
				"          category_id = sourceFilmCategory@category_id,",
				"          last_update = sourceFilmCategory@last_update,",
				"          category_id = sourceCategory@category_id,",
				"          name",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"surrogateKey1, sourceFilm lookup(category_id == sourceFilm@film_id,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"select1 keyGenerate(output(FilmKey as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"lookup1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'fsuofuis6480dldw001',",
				"     folderPath: 'IS6480_DW_GroupSix_Project/DimFilm',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: true,",
				"     updateable: false,",
				"     upsertable: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          FilmKey,",
				"          FilmID = select1@film_id,",
				"          CategoryID = category_id,",
				"          Name = name,",
				"          FilmID = sourceFilm@film_id,",
				"          Title = title,",
				"          Description = description,",
				"          RentalRate = rental_rate,",
				"          Rating = rating",
				"     )) ~> sink1"
			]
		}
	}
}